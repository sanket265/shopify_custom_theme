  /* Image slider style */
  .title h4{
    text-align:center;
  }
  .slick-slider .element{
    height:100px;
    background-color:#000;
    border-radius:5px;
    display:inline-block;
    margin:0px 10px;
    display:-webkit-box;
    display:-ms-flexbox;
    display:flex;
    -webkit-box-pack:center;
        -ms-flex-pack:center;
            justify-content:center;
    -webkit-box-align:center;
        -ms-flex-align:center;
            align-items:center;
    font-size:20px;
  }
  
  .slick-slider .element img{
      width:100%;
      -o-object-fit:contain;
         object-fit:contain;
  }
{% endstyle %}

{% javascript %}
  $(".slick-slider").slick({
   slidesToShow: 3,
   infinite:false,
   slidesToScroll: 1,
   autoplay: true,
   autoplaySpeed: 2000,
    arrows: true
     // dots: false, Boolean
    // arrows: false, Boolean
  });
{% endjavascript %}


{% schema %}
  {
    "name": "Custom slide show",
    "tag": "section",
    "class": "custom-slideshow",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Custom Slideshow"
      },
      {
        "type": "text",
        "id": "description",
        "label": "Description"
      }
    ],
    "presets": [
      {
        "name": "Custom slide show",
        "settings": {
          "title": "Slideshow"
        },
      }
    ]
  }
{% endschema %}